import bibix
from ".." import ktjvm
import maven

impl = ktjvm.library(
  srcs = glob("src/**/*.kt"),
  deps = [
    bibix.base,
  ],
)

def unzip(
  zipFile: file,
): directory = impl:com.giyeok.bibix.plugins.zip.Unzip

def gzip(
  file: file,
  outputFileName?: string,
): file = impl:com.giyeok.bibix.plugins.zip.Gzip
